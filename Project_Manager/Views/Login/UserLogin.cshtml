@{
    ViewBag.Title = "UserLogin";
    Layout = null;


}
<head>
    <link rel="icon" type="image/png" href="@Url.Content("~/Content/images/icons/favicon.ico")" />
    <link href="~/Content/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/fonts/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/fonts/iconic/css/material-design-iconic-font.css" rel="stylesheet" />
    <link href="~/Content/vendor/animate/animate.css" rel="stylesheet" />
    <link href="~/Content/vendor/css-hamburgers/hamburgers.css" rel="stylesheet" />
    <link href="~/Content/vendor/animsition/css/animsition.css" rel="stylesheet" />
    <link href="~/Content/vendor/select2/select2.css" rel="stylesheet" />
    <link href="~/Content/vendor/daterangepicker/daterangepicker.css" rel="stylesheet" />
    <link href="~/Content/css/util.css" rel="stylesheet" />
    <link href="~/Content/css/main.css" rel="stylesheet" />

    <link href="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js" />
    <link href="~/Content/Dialog/Dialog-ie.css" rel="stylesheet" />
    <link href="~/Content/Dialog/Dialog.css" rel="stylesheet" />
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>

</head>
<body>

    <div class="limiter">
        <div class="container-login100" style="background-image: url('../../Content/images/bg-01.jpg');">
            <div class="wrap-login100">
                <form class="login100-form validate-form" method="get" id="formularioInicio">
                    <span class="login100-form-logo">
                        <i class="zmdi zmdi-landscape"></i>
                    </span>

                    <span class="login100-form-title p-b-34 p-t-27">
                        Log in
                    </span>

                    <div class="wrap-input100 validate-input" data-validate="Enter username">
                        <!--USUARIO-->
                        <input class="input100" type="text" id="username" placeholder="Username">
                        <span class="focus-input100" data-placeholder="&#xf207;"></span>
                    </div>

                    <div class="wrap-input100 validate-input" data-validate="Enter password">
                        <!--CONTRASEÑA-->
                        <input class="input100" type="password" id="pass" placeholder="Password">
                        <span class="focus-input100" data-placeholder="&#xf191;"></span>
                    </div>


                    <div class="container-login100-form-btn">
                        <button type="submit" class="btn btn-success btn-lg">Ingresar</button>
                    </div>



                    <div class="text-center p-t-90">
                        <a class="" href="#">
                            Forgot Password?
                        </a>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <div id="dropDownSelect1"></div>

    <script src="~/Scripts/Login.js"></script>
    <script>
        $(function () {
            ValidadLogin.init();
        });
    </script>

    <script>
        var Root = "@Url.Content("~/")";
    </script>
    <script src="~/Content/Dialog/Dialog.js"></script>


    <script src="~/Content/vendor/jquery/jquery-3.2.1.min.js"></script>
    <script src="~/Content/vendor/animsition/js/animsition.js"></script>
    <script src="~/Content/vendor/bootstrap/js/popper.js"></script>
    <script src="~/Content/vendor/bootstrap/js/bootstrap.js"></script>
    <script src="~/Content/vendor/select2/select2.js"></script>
    <script src="~/Content/vendor/daterangepicker/daterangepicker.js"></script>
    <script src="~/Content/vendor/daterangepicker/moment.js"></script>
    <script src="~/Content/vendor/countdowntime/countdowntime.js"></script>
    <script src="~/Content/js/main.js"></script>


</body>
